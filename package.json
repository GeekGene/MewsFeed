{
  "name": "clutter-dev",
  "private": true,
  "version": "0.7.0-alpha",
  "workspaces": [
    "ui",
    "tests"
  ],
  "scripts": {
    "// 0": "DNA",
    "test": "npm run build:happ && npm t -w tests",
    "build:happ": "npm run build:dnas && hc app pack ./workdir",
    "build:dnas": "npm run build:zomes && hc dna pack ./dnas/clutter/workdir",
    "build:zomes": "CARGO_TARGET_DIR=target cargo build --release --target wasm32-unknown-unknown",
    "// 1": "UI",
    "web": "nx run clutter-web:serve",
    "start": "npm run build:happ && npm run start:agent",
    "start:agent": "cross-env HC_PORT=$(port) concurrently \"npm run playground\" \"npm run start:happ\" \"sleep 5 && nx run clutter-web:serve\"",
    "start:happ": "hc s clean && echo pass | RUST_LOG=warn hc s --piped generate ./workdir/clutter.happ --run=$HC_PORT -a clutter network mdns",
    "// 2": "TOOLS",
    "playground": "run-singleton \"holochain-playground\"",
    "// 3": "WIP CLEAN",
    "package": "npm run build:happ && npm run package:ui && hc web-app pack workdir",
    "package:ui": "npm run build -w ui && cd ui/dist && bestzip ../dist.zip *",
    "network": "npm run build:happ && concurrently-repeat \"npm run start:agent\""
  },
  "dependencies": {
    "@angular/animations": "~15.0.0",
    "@angular/common": "~15.0.0",
    "@angular/compiler": "~15.0.0",
    "@angular/core": "~15.0.0",
    "@angular/forms": "~15.0.0",
    "@angular/platform-browser": "~15.0.0",
    "@angular/platform-browser-dynamic": "~15.0.0",
    "@angular/router": "~15.0.0",
    "@holo-host/web-sdk": "^0.6.6-prerelease",
    "@holochain-open-dev/cell-client": "^0.7.1",
    "@holochain-open-dev/core-types": "^0.4.0",
    "@holochain-open-dev/profiles": "^0.4.3",
    "@holochain-open-dev/utils": "^0.3.1",
    "@holochain/client": "^0.8.0",
    "angular-in-memory-web-api": "^0.14.0",
    "moment": "^2.29.4",
    "ngx-moment": "^6.0.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.0",
    "@angular-eslint/eslint-plugin": "~14.0.4",
    "@angular-eslint/eslint-plugin-template": "~14.0.4",
    "@angular-eslint/template-parser": "~14.0.4",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "~15.0.0",
    "@angular/language-service": "~15.0.0",
    "@holo-host/chaperone": "^9.0.0",
    "@holochain-playground/cli": "^0.0.11",
    "@nrwl/angular": "^15.2.1",
    "@nrwl/cypress": "15.2.1",
    "@nrwl/jest": "15.2.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.17.0",
    "bestzip": "^2.2.0",
    "concurrently": "^6.2.1",
    "concurrently-repeat": "^0.0.1",
    "cross-env": "^7.0.3",
    "cypress": "^11.0.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.0",
    "jest": "28.1.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "~12.2.2",
    "new-port-cli": "^1.0.0",
    "nx": "15.2.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "run-singleton-cli": "^0.0.7",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "npm": ">=7.0.0"
  }
}
