    "async-retry": "^1.3.3",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "material-icons": "^1.13.6",
    "pinia": "^2.1.3",
    "pinia-plugin-persistedstate": "^3.1.0",
    "quasar": "^2.12.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
