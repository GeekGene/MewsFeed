    "lodash.min": "^4.0.1",
    "lodash.union": "^4.6.0",
    "pinia": "^2.0.23",
    "quasar": "^2.9.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5"
