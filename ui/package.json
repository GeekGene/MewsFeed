{
  "name": "mewsfeed-ui-vue",
  "description": "A UI for MewsFeed based on Vue",
  "author": "Jost Schulte",
  "version": "0.9.0-alpha",
  "type": "module",
  "scripts": {
    "start": "vite --port $UI_PORT --clearScreen false",
    "start:browser": "VITE_HC_PORT=$HC_PORT VITE_HC_ADMIN_PORT=$HC_ADMIN_PORT vite",
